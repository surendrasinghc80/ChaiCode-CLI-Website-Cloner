{
  "name": "chaicode-ai-agent-cli",
  "version": "1.0.0",
  "description": "CLI to clone a website for fully offline use (HTML/CSS/JS/assets) with link rewriting and optional local server.",
  "bin": {
    "siteclone": "bin/siteclone.js"
  },
  "main": "src/server.js",
  "scripts": {
    "build": "echo 'No build step required'",
    "start": "node bin/siteclone.js",
    "demo:vscode": "node bin/siteclone.js https://code.visualstudio.com --out dist/vscode --max-pages 300 --serve 4173 --open",
    "demo:github": "node bin/siteclone.js https://github.com --out dist/github --max-pages 500 --serve 4174 --open",
    "demo:piyush": "node bin/siteclone.js https://piyushgarg.dev --out dist/piyush --max-pages 50 --serve 4175 --open",
    "demo:codeoptimalsolutions": "node bin/siteclone.js https://codeoptimalsolutions.com/ --out dist/codeoptimalsolutions --max-pages 50 --serve 4176 --open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "cheerio": "^1.0.0",
    "express": "^4.19.2",
    "glob": "^11.0.0",
    "mime-types": "^2.1.35",
    "node-html-parser": "^6.1.13",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "p-limit": "^6.0.0",
    "postcss": "^8.4.47",
    "postcss-url": "^10.1.3",
    "yargs": "^17.7.2",
    "got": "^14.4.2"
  }
}
